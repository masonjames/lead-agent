# Vercel AI Gateway API Key (https://vercel.com/ai-gateway)
AI_GATEWAY_API_KEY=

# Exa API Key (https://exa.ai/)
EXA_API_KEY=

# Resend API Key (https://resend.com)
# Required for email reports
RESEND_API_KEY=

# Email configuration
# The email address to send lead reports to
REPORT_TO_EMAIL=masonjames@gmail.com

# The "from" email address for reports (must be verified in Resend)
REPORT_FROM_EMAIL=leads@notifications.example.com

# Meta (Facebook) Lead Ads Webhook
# Required for Facebook Lead Ads integration
META_APP_SECRET=
META_ACCESS_TOKEN=
META_VERIFY_TOKEN=

# Playwright configuration (required for PAO property scraping)
# Default mode uses stealth CDP (SeleniumBase or similar)
PLAYWRIGHT_MODE=stealth

# Canonical CDP endpoint (DevTools / remote debugging URL)
# Examples:
#   http://127.0.0.1:9222
#   ws://127.0.0.1:9222/devtools/browser/<id>
PLAYWRIGHT_CDP_ENDPOINT=

# Set PLAYWRIGHT_MODE=local to use local Chromium in development (not recommended for prod)

# Deprecated alias (still supported for backward compatibility)
PLAYWRIGHT_WS_ENDPOINT=

# StellarMLS / Realist enrichment
STELLARMLS_PING_AUTHORIZE_URL=
STELLARMLS_USERNAME=
STELLARMLS_PASSWORD=

# Session reuse (Playwright storageState)
# Either provide JSON directly or base64 JSON
STELLARMLS_STORAGE_STATE_JSON=
STELLARMLS_STORAGE_STATE_B64=

# Optional: pull session state from an external service (JWT-based)
STELLARMLS_SESSION_INFO_URL=
STELLARMLS_SESSION_INFO_JWT=

# Optional: persist refreshed session in Neon
STELLARMLS_SESSION_DB_ENABLED=false
STELLARMLS_SESSION_ACCOUNT_KEY=

# Database (Neon PostgreSQL)
# Required for parcel data ingestion and storage
# Get your connection string from https://neon.tech/
DATABASE_URL=

# Parcel Ingestion Configuration (optional)
# Set to "true" to enable automatic parcel storage during lead enrichment
PARCEL_INGESTION_ENABLED=false

# Default parcel source (optional)
# Valid values: fl-manatee-pa (more sources coming soon)
PARCEL_DEFAULT_SOURCE=fl-manatee-pa
